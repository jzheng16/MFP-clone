{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "react/prop-types": 0,
    "camelcase": 0,
    "max-len": 0,
    "indentSwitchCase": true,
    "react/self-closing-comp": 0,
    "no-console": 0,
    "class-methods-use-this": 1,
    "linebreak-style": 0,
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-unused-vars": 1,
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}